{
  "expo": {
    "name": "GSB Application",
    "slug": "GSBApplicationMobile",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.leandreGuegan.GSBApplication"
    },
    "android": {
      "package": "com.leandreGuegan.GSBApplication"
    },
    "githubUrl": "https://github.com/lelou27/GSBApplicationMobile"
  }
}